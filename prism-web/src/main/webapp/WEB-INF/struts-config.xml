        <!-- Vendor Form-->
        <form-bean name="vendorsForm" type="us.mn.state.health.view.common.VendorsForm"/>
        
        <!-- Reporting Forms -->
        <form-bean name="generalReportingForm" type="us.mn.state.health.view.reporting.GeneralReportingForm"/>

        <form-bean name="luceneIndexesForm" type="us.mn.state.health.model.util.search.struts.LuceneIndexesForm"/>

        <form-bean name="CachedObjectsForm" type="us.mn.state.health.web.struts.admin.CachedObjectsForm"/>

        <form-bean name="BudgetBuilderForm" type="us.mn.state.health.web.struts.admin.BudgetBuilderForm"/>

         <form-bean name="purchasereport" type="us.mn.state.health.view.materialsrequest.reports.FishConsumptionForm"/>

    </form-beans>

    <action-mappings>

        <action path="/purchasereport"
                type="us.mn.state.health.view.materialsrequest.reports.FishConsumptionAction"
                name="purchasereport"
                scope="session"
                parameter="generateReport"
                validate="false">
            <!-- <forward name="success" path="/requests/reports/index.jsp"/> -->
            
            
        </action>

        
       <action path="/viewAddNewAddressStep1"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="viewAddNewAddressStep1"
                validate="false">
            <forward name="success" path="/common/viewAddNewAddressStep1.jsp"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>
        <action path="/viewAddNewAddressStep2"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="viewAddNewAddressStep2"
                input="/common/viewAddNewAddressStep1.jsp"
                validate="true">
            <forward name="step2" path="/common/viewAddNewAddressStep2.jsp"/>
            <forward name="step3" path="/viewAddNewAddressStep3.do" redirect="true"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>
        <action path="/viewAddNewAddressStep3"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="viewAddNewAddressStep3"
                input="/common/viewAddNewAddressStep1.jsp"
                validate="false">
            <forward name="success" path="/common/viewAddNewAddressStep3.jsp"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>
        <action path="/addNewAddressFinalStep"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="addNewAddressFinalStep"
                input="/common/viewAddNewAddressStep3.jsp"
                validate="false">
            <forward name="success" path="/viewAddNewAddressStep1.do" redirect="true"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>
        <action path="/addContacts"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="addContacts"
                validate="false">
            <forward name="success" path="/common/viewAddNewAddressStep2.jsp"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>
        <action path="/viewAddPerson"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="viewAddPerson"
                validate="false">
            <forward name="success" path="/common/viewAddPerson.jsp"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>
        <action path="/addPerson"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="addPerson"
                input="/common/viewAddPerson.jsp"
                validate="true">
            <forward name="success" path="/common/viewAddNewAddressStep2.jsp"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>
        <action path="/viewAddNewMailingAddress"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="viewAddNewMailingAddress"
                validate="false">
            <forward name="success" path="/common/viewAddNewMailingAddress.jsp"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>
        <action path="/addNewMailingAddress"
                type="us.mn.state.health.matmgmt.action.DeliveryAddressAction"
                name="deliveryAddressForm"
                scope="session"
                parameter="addNewMailingAddress"
                input="/common/viewAddNewMailingAddress.jsp"
                validate="true">
            <forward name="success" path="/common/viewAddNewAddressStep3.jsp"/>
            <forward name="failure" path="/common/error.jsp"/>
        </action>

        <action path="/admin"
            type="us.mn.state.health.web.struts.admin.EvictCacheAction"
            name="CachedObjectsForm"
            scope="session"
            parameter="admin"
            validate="false">
            <forward name="success" path="/common/admin.jsp"/>
            <forward name="notAuthorized" path="/common/unAuthorizedAttempt.jsp"/>
        </action>

        <action path="/selectIndexes"
                type="us.mn.state.health.model.util.search.struts.LuceneIndexCreatorAction"
                name="luceneIndexesForm"
                scope="session"
                parameter="selectIndexes"
                validate="false">
            <forward name="success" path="/common/selectIndexes.jsp"/>
        </action>

        <action path="/buildIndexes"
                type="us.mn.state.health.model.util.search.struts.LuceneIndexCreatorAction"
                name="luceneIndexesForm"
                scope="session"
                parameter="buildIndexes"
                validate="false">
            <forward name="success" path="/common/buildIndexes.jsp"/>
        </action>

        <action path="/selectCacheToEvict"
                type="us.mn.state.health.web.struts.admin.EvictCacheAction"
                name="CachedObjectsForm"
                scope="session"
                parameter="selectCacheToEvict"
                validate="false">
            <forward name="success" path="/common/selectCacheToEvict.jsp"/>
            <forward name="notAuthorized" path="/common/unAuthorizedAttempt.jsp"/>
        </action>

        <action path="/evictCache"
                type="us.mn.state.health.web.struts.admin.EvictCacheAction"
                name="CachedObjectsForm"
                scope="session"
                parameter="evictCache"
                validate="false">
            <forward name="success" path="/common/evictCache.jsp"/>
            <forward name="notAuthorized" path="/common/unAuthorizedAttempt.jsp"/>
        </action>

        <action path="/changePrismNotifications"
                type="us.mn.state.health.web.struts.admin.EvictCacheAction"
                name="CachedObjectsForm"
                scope="session"
                parameter="changePrismNotifications"
                validate="false">
            <forward name="success" path="/common/changePrismNotifications.jsp"/>
            <forward name="notAuthorized" path="/common/unAuthorizedAttempt.jsp"/>
        </action>

        <action path="/changePrismNotificationsSave"
                type="us.mn.state.health.web.struts.admin.EvictCacheAction"
                name="CachedObjectsForm"
                scope="session"
                parameter="changePrismNotificationsSave"
                validate="false">
            <forward name="success" path="/common/admin.jsp"/>
            <forward name="notAuthorized" path="/common/unAuthorizedAttempt.jsp"/>
        </action>

        <action path="/changeBudgetBuilderLink"
                type="us.mn.state.health.web.struts.admin.BudgetBuilderAction"
                name="BudgetBuilderForm"
                scope="session"
                parameter="changeBudgetBuilderLink"
                validate="false">
            <forward name="success" path="/common/changeBudgetBuilderLink.jsp"/>
            <forward name="notAuthorized" path="/common/unAuthorizedAttempt.jsp"/>
        </action>

        <action path="/changeBudgetBuilderLinkSave"
                type="us.mn.state.health.web.struts.admin.BudgetBuilderAction"
                name="BudgetBuilderForm"
                scope="session"
                parameter="changeBudgetBuilderLinkSave"
                validate="false">
            <forward name="success" path="/common/admin.jsp"/>
            <forward name="notAuthorized" path="/common/unAuthorizedAttempt.jsp"/>
        </action>

    </action-mappings>

    <message-resources parameter="ApplicationResources"/>

    <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
        <set-property property="pathnames" value="/WEB-INF/validator-rules.xml,
        /WEB-INF/validation.xml,/WEB-INF/inventory/validation-inventory.xml,
        /WEB-INF/purchasing/validation-purchasing.xml,/WEB-INF/receiving/validation-receiving.xml,
        /WEB-INF/requests/validation-requests.xml"/>
    </plug-in>
    <plug-in className="net.sf.navigator.menu.MenuPlugIn">
            <set-property property="menuConfig" value="/WEB-INF/menu-config.xml"/> <!-- Default settings -->
    </plug-in>
</struts-config>
