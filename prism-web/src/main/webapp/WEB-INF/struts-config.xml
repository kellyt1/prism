        <form-bean name="searchCatalogForm" type="us.mn.state.health.view.materialsrequest.SearchCatalogForm"/>
        <form-bean name="itemDetailsForm" type="us.mn.state.health.view.materialsrequest.ItemDetailsForm"/>
        <form-bean name="advancedSearchCatalogForm" type="us.mn.state.health.view.inventory.ItemsAdvancedSearchForm"/>
        <form-bean name="searchMaterialRequestsForm" type="us.mn.state.health.view.materialsrequest.SearchMaterialRequestsForm"/>
