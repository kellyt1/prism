        <form-bean name="requestLineItemNoteForm" type="us.mn.state.health.view.materialsrequest.RequestLineItemNoteForm"/>
        <form-bean name="browseCatalogForm" type="us.mn.state.health.view.materialsrequest.BrowseCatalogForm"/>
