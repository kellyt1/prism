        <form-bean name="stockItemOutTransactionsForm" type="us.mn.state.health.view.materialsrequest.reports.StockItemTransactionsForm"/>
        <form-bean name="stockItemOutTransactionsWithoutFSForm" type="us.mn.state.health.view.materialsrequest.reports.StockItemTransactionsForm"/>
        <form-bean name="stockItemInTransactionsForm" type="us.mn.state.health.view.materialsrequest.reports.StockItemTransactionsForm"/>
        <form-bean name="purchaseItemTransactionsForm" type="us.mn.state.health.view.materialsrequest.reports.PurchaseItemTransactionsForm"/>

        <!-- Purchasing Forms -->
        <form-bean name="orderForm" type="us.mn.state.health.view.purchasing.OrderForm"/>
