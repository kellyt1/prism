        <form-bean name="advancedSearchStockItemsForm" type="us.mn.state.health.view.inventory.ItemsAdvancedSearchForm"/>
        <form-bean name="stockItemReorderRequestForm" type="us.mn.state.health.view.materialsrequest.RequestForm"/>
        <form-bean name="stockItemReorderRequestRLIForm" type="us.mn.state.health.view.materialsrequest.RequestLineItemForm"/>
        <form-bean name="stockItemsReportForm" type="us.mn.state.health.view.inventory.StockItemsReportForm"/>
        
        <!-- Materials Request Forms -->
        <form-bean name="deliveryDetailForm" type="us.mn.state.health.view.materialsrequest.DeliveryDetailForm"/>
