        <form-bean name="stockItemActionRequestsForm" type="us.mn.state.health.view.inventory.StockItemActionRequestsForm"/>

        <!-- Stock Item Forms -->
        <form-bean name="stockItemForm" type="us.mn.state.health.view.inventory.StockItemForm"/>
